@media (min-width: 1400px) {
    .container {
        max-width: 1300px;
    }
}

ul {
    margin: 0;
    padding: 0;
}
h5 {
  margin: 0;
}
.count-head {
    height: 102px;
    text-align: center;
}

.count-head h5 {
    line-height: 102px;
    font-size: 20px;
}

.count-main .time-select {
    height: 42px;
    border-top: 1px solid #dedede;
    background-color: #f7f7f7;
}

.count-main .parent-width {
    width: 100%;
    margin-top: 15px;
}

.parent-width .width50 {
    float: left;
    width: 50%;
}

.echarts-one,
.echarts-two {
    margin-right: 10px;
    padding: 0 20px;
    height: 320px;
    border: 1px solid #ddd;
}

.echarts-two {
    margin-right: 0;
    margin-left: 10px;
}

.echarts-head {
    height: 43px;
    border-bottom: 1px solid #ddd;
}

.mt18 {
    margin-top: 18px;
}

.echarts-body .left-countList {
    float: left;
    border-right: 1px solid #ddd;
    width: 40%;
}

.left-countList ul {
    padding-right: 18px;
}

.left-countList li {
    height: 48px;
    line-height: 48px;
    margin-bottom: 13px;
    background-color: #f5fafd;
    color: #1363b8;
    border-left: 2px solid #1363b8;
}

.left-countList li.mb0 {
    margin-bottom: 0;
}

.right-countechart {
    position: relative;
    float: left;
    width: 60%;
}

.right-countechart .echart1 {
    margin-left: 20px;
    height: 230px;
}

.right-countechart .echart-content {
    position: absolute;
    top: 0;
    left: 20px;
    height: 230px;
    width: 100%;
}

.echart-content .count1 {
    height: 114px;
    margin-left: 20px;
    border-bottom: 1px solid #ddd;
}

.echart-content .count2 {
    margin-left: 20px;
}


/* 第二栏图表width57*/

.parent-width.border {
    border: 1px solid #ddd;
}

.parent-width .width57 {
    float: left;
    width: 57%;
}

.parent-width .width43 {
    float: left;
    width: 43%;
}

.echarts-three,
.echarts-four,
.echarts-five,
.echarts-six {
    padding: 0 20px;
    height: 330px;
}
.echarts-seven,
.echarts-eight {
    padding: 0 20px;
    height: 350px;
}
.echarts-body .echart5,
.echarts-body .echart6,
.echarts-body .echart3,
.echarts-body .echart4 {
  height: 290px;
}
.left-countcontrast li {
    height: 39px;
    line-height: 39px;
    margin-bottom: 10px;
    background-color: #f5fafd;
    color: #1363b8;
    border-left: 2px solid #1363b8;
}
.left-countcontrast li.mb0 {
  margin-bottom: 0;
}

.parent-width .width28 {
    float: left;
    width: 28%;
}

.parent-width .width72 {
    float: left;
    width: 72%;
}

/*第三栏图标*/
.month-classic {
  height: 50px;
  line-height: 50px;
  text-align: right;
  color: #1264b8;
}

.month-classic span {
  display: inline-block;
  padding: 0 20px 0 18px;
}

.echarts-body .echart7,
.echarts-body .echart8 {
  position: relative;
  float: left;
  width: 50%;
  height: 305px;
}
