//首页模块方法
var getSingleCount = function(res,id) {
        return parseInt($(res).find(id).text());
}
//问题总数
var getProblemAll = function (res) {
       return getSingleCount(res,'ZWFWcountall') +getSingleCount(res,'DZZWcountall') +getSingleCount(res,'SZJScountall') +getSingleCount(res,'JZQYcountall') +getSingleCount(res,'JCZCcountall');
}
//已经结案问题总数
var getProblemDoneAll = function (res) {
       return getSingleCount(res,'ZWFWcountYJA') +getSingleCount(res,'DZZWcountYJA') +getSingleCount(res,'SZJScountYJA') +getSingleCount(res,'JZQYcountYJA') +getSingleCount(res,'JCZCcountYJA');
}
//未结案问题总数
var getProblemUnDoneAll = function (res) {
       return getSingleCount(res,'ZWFWcountDJA') +getSingleCount(res,'DZZWcountDJA') +getSingleCount(res,'SZJScountDJA') +getSingleCount(res,'JZQYcountDJA') +getSingleCount(res,'JCZCcountDJA');
}
//疑难问题总数
var getDifficulteAll = function (res) {
       return getSingleCount(res,'ZWFWYN') +getSingleCount(res,'DZZWYN') +getSingleCount(res,'SZJSYN') +getSingleCount(res,'JZQYYN') +getSingleCount(res,'JCZCYN');
}
//平均处理时长
var getAverageTime = function (res) {
       return (getSingleCount(res,'ZWFWHour') +getSingleCount(res,'DZZWHour') +getSingleCount(res,'SZJSHour') +getSingleCount(res,'JZQYHour') +getSingleCount(res,'JCZCHour'))/5;
}
