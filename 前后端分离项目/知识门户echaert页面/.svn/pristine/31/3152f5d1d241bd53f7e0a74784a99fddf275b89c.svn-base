    <!--模板 t仪表盘-->
    <script type="text/x-template" id="tmpl1">
        <div class="echarts-head">
            <h5>{{title}}</h5>
        </div>
        <div class="echarts-body mt18 clearfix">
            <div class="left-countList">
                <ul>
                    <li class="clearfix">
                        <span class="name">问题统计</span>
                        <span class="num"><em>{{wttj}}</em>个</span>
                    </li>
                    <li>
                        <span class="name">已结案问题</span>
                        <span class="num"><em>{{yjawt}}</em>个</span>
                    </li>
                    <li>
                        <span class="name">未结案问题</span>
                        <span class="num"><em>{{wjawt}}</em>个</span>
                    </li>
                    <li class="mb0">
                        <span class="name">疑难问题</span>
                        <span class="num"><em>{{ynwt}}</em>个</span>
                    </li>
                </ul>
            </div>
            <div class="right-countechart">
                <div class="echart1" id="{{echart}}">
                </div>
                <div class="echart-content">
                    <div class="count1 clearfix">
                        <h6>问题总结案率:</h6>
                        <p>{{wtzjal}}%</p>
                    </div>
                    <div class="count2 clearfix">
                        <h6>问题总平均办理时长:</h6>
                        <p class="p1"><span>{{wttime1}}</span>:<span>{{wttime2}}</span>:<span>00</span></p>
                        <p class="p2"><span>hours</span> <span>minutes</span><span>seconds</span></p>
                    </div>
                </div>
            </div>
        </div>
    </script>
