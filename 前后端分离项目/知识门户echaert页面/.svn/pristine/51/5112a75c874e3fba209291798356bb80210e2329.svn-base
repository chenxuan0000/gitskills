.quality-improve {
  width: 820px;
}

.count-block {
  margin-top: 15px;
  border: 1px solid #ddd;
  border-right: 0;
}

.count-block ul {
  padding: 0;
  margin: 0;
}
.count-block li {
  float: left;
  margin: 0;
  width: 163.8px;
  height: 78px;
  border-right: 1px solid #ddd;
}

/*echart 区域*/
.left-echart,
.right-echart {
  float: left;
  width: 478px;
  height: 304px;
  margin-top: 15px;
  border: 1px solid #ddd;
}

.right-echart {
  float: right;
  width: 321px;
}