.main {
    overflow: hidden;
    min-height: 400px;
    border-top: 1px solid #c3cdda;
    background: url(../images/detail-bg.jpg) center top no-repeat;
}
.now-pos {
    height: 50px;
    line-height: 50px;
    padding-left: 32px;
    background: url(../images/now-pos.png) 12px center no-repeat;
}
.now-pos>span,
.now-pos>a {
    display: inline-block;
    margin: 0 8px;
    overflow: hidden;
}
.now-pos>span.gt {
    margin: 0;
}
.now-pos>.cur {
    color: #d80000;
    width: 400px;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.intro-detail {
    min-height: 310px;
    margin-bottom: 15px;
    border: 1px solid #d1d1d1;
    background-color: #fff;
}

.intro-detail .left {
    float: left;
    width: 406px;
    height: 266px;
    margin-left: 20px;
    margin-top: 20px;
    border: 1px solid #d1d1d1;
}

.intro-detail .left>img {
    width: 406px;
    height: 266px;
}

.intro-detail .right {
    float: left;
    width: 510px;
    margin-left: 25px;
    margin-top: 15px;
}

.item-name {
    /*max-height: 48px;*/
    height: 48px;
    overflow: hidden;
    font-size: 16px;
    line-height: 24px;
}
.item-label {
    margin: 6px 0 11px 0;
}
.item-label>.blue {
    display: inline-block;
    color: #0068b7;
    margin-right: 6px;
}

.right-block {
    border: 1px solid #d1d1d1;
}

.right-block li {
    height: 30px;
    line-height: 30px;
}
.right-block li>.one {
    float: left;
    width: 87px;
    height: 30px;
    text-align: center;
    background-color: #f2f2f2;
}

.right-block li>.two {
    float: left;
    width: 400px;
    height: 30px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-left: 15px;
}

/**/
.look>a {
    display: inline-block;
    margin-top: 7px;
    margin-left: 10px;
    height: 16px;
    cursor: pointer;
    background-image: url(../images/small-icon.png);
}

.look>.computer {
    width: 19px;
    background-position: -44px 0;
}

.look>.computer:hover {
    background-position: -63px 0;
}

.look>.file {
    width: 12px;
    background-position: -32px 0;
}

.look>.file:hover {
    background-position: -20px 0;
}

.look>.tel {
    width: 10px;
    margin-top: -5px;
    background-position: -10px 0;
}

.look>.tel:hover {
    background-position: 0 0;
}

.look>.tel:hover .code {
    display: block;
}

.name,
.num {
    width: 280px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis
}

.num {
    width: 180px;
}


/**/
.right-download {
    margin-top: 20px;
    height: 44px;
    line-height: 44px;
}

.right-download>.down {
    display: inline-block;
    width: 185px;
    height: 44px;
    margin-right: 19px;
    color: #fff;
    font-size: 16px;
    font-family: Microsoft YaHei;
    border: 0;
    background-color: #0068b7;
    text-align: center;
    font-weight: bold;
    cursor: pointer;
}
.right-download>.down:hover {
    background-color: #d80000;
}
.right-download>span {
    color: #666;
}

/*图片展示*/
.img-list em {
   font-style:italic;
}
.img-list li {
    margin-bottom: 10px;
    text-align: center;
}

img.loadImg {
    max-width: 996px;
    border: 1px solid #e1e1e1;
}

.bigFile-download {
    width: 998px;
    margin-top: 15px;
    min-height: 50px;
    background-color: #f0f0f0;
}
#copyDown {
    position: fixed;
    bottom: 0;
    left: 50%;
    margin-left: -499px;
}

.left-list {
    float: left;
    width: 780px;
}
.left-list li {
    float: left;
    margin-top: 5px;
    line-height: 38px;
    margin-left: 18px;
}
.left-list .name-cur {
    margin-left: 14px;
    font-size: 16px;
    color: #de2424;
}
.left-list li>span {
    white-space: nowrap;
    cursor: pointer;
}

.all-dn {
    float: left;
    margin-top: 5px;
    margin-left: 18px;
    width: 180px;
    height: 34px;
    line-height: 34px;
    font-size: 16px;
    color: #0068b7;
    font-weight: bold;
    text-align: center;
    font-family: Microsoft YaHei;
    background-color: #fff;
    border: 3px solid #0068b7;
    cursor: pointer;
}
.all-dn:hover {
    border-color: #d80000;
    color: #d80000;
}


/*pre-next*/
.pre-next {
    height: 40px;
    line-height: 40px;
}

.pre-next>.pre,
.pre-next>.next {
    float: left;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 400px;
    overflow: hidden;
    color: #0068b7;
}

.pre-next>.next {
    float: right;
    text-align: right;
}
/*back-top*/
.back-top {
    position: fixed;
    display: none;
    bottom: 120px;
    right: 100px;
    width: 47px;
    height: 47px;
    cursor: pointer;
    background: url(../images/back-top.png) center no-repeat #0068b7;
}

.back-top:hover {
    background-color: #d80000;
}

.ht50 {
    min-height: 50px;
}