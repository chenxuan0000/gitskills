<!DOCTYPE html>
<html lang="zh-CN">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="renderer" content="webkit">
    <title>案例库</title>
    <script src="../../js/cssboot.js"></script>
    <script>
    SrcBoot.output([
        'js/lib/pagination/mricode.pagination.css',
        './css/case.css'
    ]);
    </script>
</head>

<body>
    <!-- 头 -->
    <div id="header" class="header container"></div>
    <!-- 内容 -->
    <div class="main">
        <div class="container">
            <div class="filter-block" id="filter">
                <!-- 业务 -->
                <div class="service clearfix">
                    <span class="name">业务：</span>
                    <ul class="clearfix">
                    </ul>
                </div>
                <!-- 类型 -->
                <div class="type clearfix">
                    <span class="name">类型：</span>
                    <ul class="clearfix">
                    </ul>
                </div>
                <!-- 方向 -->
                <div class="direction clearfix">
                    <span class="name">方向：</span>
                    <ul class="clearfix">
                    </ul>
                </div>
                <!-- 色调 -->
                <div class="color clearfix">
                    <span class="name">色调：</span>
                    <ul class="clearfix">
                    </ul>
                </div>
                <!-- 成果 -->
                <div class="achievement" id="checkbox">
                </div>
            </div>
            <!-- 跟随滚动层 -->
            <div class="search-block clearfix" id="search">
                <div class="lastest order" id="num">最新</div>
                <div class="down-num order" id="down">下载量</div>
                <div class="search-input clearfix">
                    <input type="text">
                    <button></button>
                </div>
                <div class="smallPage clearfix" id="smallpager">
                </div>
            </div>
            <!-- 查询展示列表 -->
            <div class="result-list">
                <ul id="result-ul" class="clearfix">
                </ul>
                <div class="pager clearfix" id="pager"></div>
            </div>
        </div>
    </div>
    <!-- 尾 -->
    <div id="footer" class="footer"></div>
    <!--模板-->
    <script type="text/x-template" id="tmpl">
        <li data-type="" class="selected">全部</li>
        {{#Info}}
        <li data-type="{{Value}}">{{Name}}</li>
        {{/Info}}
    </script>
    <script type="text/x-template" id="tmpl1">
        <li data-type="" class="selected">全部</li>
        {{#Info}}
        <li data-type="{{CategoryNum}}">{{CategoryName}}</li>
        {{/Info}}
    </script>
    <!-- 成果 -->
    <script type="text/x-template" id="tmpl2">
        <span class="blue">成果：</span> {{#Info}}
        <label>
            <input name="achieve" type="checkbox" value="{{Value}}" />{{Name}}</label>
        {{/Info}}
    </script>
    <!-- 色值 -->
    <script type="text/x-template" id="tmpl3">
        <li data-type="" class="all selected">全部</li>
        {{#Info}}
        <li class="{{color}}" data-type="{{Value}}" style="background-color:#{{color}}"></li>
        {{/Info}}
    </script>
    <!-- 筛选结果列表 -->
    <script type="text/x-template" id="tmpl4">
        {{#Info}}
        <li>
            <div class="img-div">
                <a href="javascript:void(0);">
                    <img data-id={{InfoGuid}} src="{{ListImgUrl}}" alt="加载图片失败" class="show-img"></a>
            </div>
            <div class="show-info">
                <p class="name" title="{{Title}}">{{Title}}</p>
                <p class="num" title="项目编号：{{ProjectNum}}">项目编号：{{ProjectNum}}</p>
                <div class="look">
                    {{#FormalWebUrl}}
                    <a target="_blank" href="{{FormalWebUrl}}" title="浏览上线网站" class="computer"></a>
                    {{/FormalWebUrl}} {{#APPImgUrl}}
                    <a class="tel">
                        <div class="code">
                            <div class="img-div">
                                <img src="{{APPImgUrl}}" alt="加载图片失败">
                            </div>
                            <span>安装移动端产品</span>
                        </div>
                    </a>
                    {{/APPImgUrl}} {{#TestWebUrl}}
                    <a target="_blank" href="{{TestWebUrl}}" title="访问测试demo" class="file"></a>
                    {{/TestWebUrl}}
                </div>
            </div>
        </li>
        {{/Info}}
    </script>
    <script src="../../js/jsboot.js"></script>
    <script>
    SrcBoot.output([
        'js/lib/pagination/mricode.pagination.js',
        './js/case.js'
    ]);
    </script>
</body>

</html>
